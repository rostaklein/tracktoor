{
  "name": "tracktoor",
  "version": "1.0.0",
  "description": "Order tracker fullstack app",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html --open",
    "build": "parcel build src/index.html --no-source-maps",
    "clean": "rimraf ./dist",
    "postinstall": "npm run generate",
    "generate": "npm run generate:prisma && npm run generate:nexus && npm run generate:fe-types",
    "generate:prisma": "prisma generate",
    "generate:nexus": "NODE_ENV=development ts-node -P nexus.tsconfig.json --transpile-only src/backend/schema",
    "generate:fe-types": "apollo client:codegen ./src/client/types.ts --outputFlat --addTypename --localSchemaFile=./src/schema.graphql --target=typescript",
    "extract:translations": "i18next",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm run lint:tsc && npm run lint:eslint",
    "lint:tsc": "tsc",
    "lint:eslint": "eslint './{src,api}/**/*.{ts,tsx}'",
    "lint:fix": "eslint './{src,api}/**/*.{ts,tsx}' --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import-alias": "^1.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "i18next-parser": "^3.7.0",
    "less": "^4.1.1",
    "parcel-bundler": "^1.12.5",
    "prettier-eslint": "^12.0.0",
    "prisma": "^2.19.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@prisma/client": "2.18.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "^26.0.21",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@vercel/node": "^1.9.1",
    "antd": "^4.14.0",
    "apollo": "^2.32.5",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-server-micro": "^2.21.2",
    "bcryptjs": "^2.4.3",
    "formik": "^2.2.6",
    "graphql": "^15.5.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^6.0.4",
    "graphql-shield": "^7.5.0",
    "graphql-tag": "^2.11.0",
    "i18next": "^19.9.2",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.32.0",
    "react": "^17.0.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.10",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  }
}
